<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logcathealth">

    <application
        android:allowBackup="true"
        android:icon="@drawable/hackathon_logo_2"
        android:label="LogCat Health"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.logcathealth">
        <activity android:name=".BottomNavigation"/>
        <activity android:name=".DocActivity" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".NormalActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>